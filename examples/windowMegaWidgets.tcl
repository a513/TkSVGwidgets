proc createWidget {can type} {
  global Rectangle Graphics
  set Graphics(type) "SVG"
  set Graphics(shape) Rectangle

  bind $can <Button-1> {
	global Graphics
set w ".c"
	set delx [winfo fpixels $w 25m]
	set dely [winfo fpixels $w 7m]
       set x [.c canvasx %x $Graphics(grid,snap)]
       set y [.c canvasy %y $Graphics(grid,snap)]
       set Rectangle(coords) "$x $y $x $y"
if {0} {
	set fillstyle [TP_opacityFromStyle $Graphics(fill,style)]
	set linestyle [TP_opacityFromStyle $Graphics(line,style)]
	set Rectangle(options)  [list \
	    -strokelinejoin miter \
            -strokewidth   $Graphics(line,width) \
            -strokedasharray    $Graphics(linesvg,dash) \
            -stroke $Graphics(line,color) \
            -fill    $Graphics(fill,color) \
            -fillopacity $fillstyle -strokeopacity $linestyle \
            -tags    {Rectangle obj svg}       \
       ]
}
#set bb [button .c.$type[expr {int($x)}] -text "buttonUX"]
set bb [button .c.widg[expr {int($x)}] -text "buttonUX"]
set winw [.c create window $x $y]
.c itemconfigure $winw -window $bb -tag "window obj utag$winw" -width $delx -height $dely -anchor nw

#       Message "Press key mouse for create widget!"
       puts "Press key mouse for create widget!"
if {0} {
      set cbut [cbutton new ".c" -type round -x $x -y $y -text crbutton]
#      $cbut config -text "cbutton" -rx 0
	History add "cbutton create $cbut .c -type round  -x $x -y $y -text crbutton -text ccbutton"
#	Undo add ".c delete $utag"
	Undo add "$cbut destroy"
	catch {unset Rectangle}
}
#	Message "Your widget  $bb  is done! Utag=utag$winw"
	puts "Your widget  $bb  is done! Utag=utag$winw"
	bind .c <Button-1> {}

  }
    bind $can <Button-3> {}
    bind $can <B1-Motion> {}
#  Message "Click on one of the vertices of the widget"
#  puts "Click on one of the vertices of the widget"
    puts "Вы хотите создать виджет.\nУкажите место его размещения"
}


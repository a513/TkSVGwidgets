proc winMegaWidgets {can {base ".widgets"}} {
    puts "winMegaWidgets: base=$base"
    set orderTK {
        Frame
        Canvas
        Button
        Entry
        Label
	"Message Box"
        Listbox
        Text
        "Menu Button"
        Menu
        "Vertical Scale"
	"Horizontal Scale"
	"Vertical Scrollbar"
	"Horizontal Scrollbar"
        "Check button"
        "Radio button"
        "Top level"
        "Label Frame"
        Spinbox
        "Horizontal Panedwindow"
        "Vertical Panedwindow"
    }
set fe1 fe_up
set fe2 fe_previous
#set fe3 fe_previousbw
set fe3 fe_gohome
    if {[winfo exists $base]} {
	wm deiconify $base
	return
    }
#    vTcl:toplevel $base -width 237 -height 110 -class vTcl
    toplevel $base 
    wm iconphoto $base tkguipaint_icon
#    -width 237
#     -height 110 
#    wm transient $base .vTcl
    wm withdraw $base
    wm title $base "Widget Toolbar"
    set ww [winfo pixels $base 15c]
    set hw [expr $ww / 3]
    wm geometry $base $ww\x$hw+110+110
    wm minsize $base $ww $hw
#    wm overrideredirect $base 0
#    catch {wm geometry .vTcl.toolbar $vTcl(geometry,.vTcl.toolbar)}
    wm deiconify $base

    tkp::canvas $base.c -width $ww -height $hw -background white
    pack $base.c -expand yes -fill y -side top
set idfe [$base.c create path "M 424.71448 528.79074 L 424.71448 529.79074 L 424.71448 544.79074 L 425.71448 544.79074 L 440.71448 544.79074 L 440.71448 543.79074 L 440.71448 530.79074 L 433.72229 530.79074 L 431.72229 528.79074 L 431.71429 528.7987400000001 L 431.71429 528.79074 L 425.71429 528.79074 L 424.71429 528.79074 Z M 425.71448 529.79074 L 428.71448 529.79074 L 431.30042 529.79074 L 432.30431999999996 530.79465 L 428.28479 534.79074 L 428.28279 534.79074 L 425.71443 534.79074 L 425.71443 529.79074 Z M 430.72229 533.79074 L 439.71448 533.79074 L 439.71448 543.79074 L 425.71448 543.79074 L 425.71448 535.79074 L 427.27698 535.79074 L 428.71448 535.79074 L 428.71448 535.78274 L 428.72247999999996 535.79074 L 430.72247999999996 533.79074 Z" -fill "#3daee9" -matrix {{1.0 0.0} {-0.0 1.0} {-350.0 -482.0}} -stroke {}]
$base.c itemconfigure $idfe  -tags {Path obj svg utag$idfe}
#set fe2 ".widgets.c [$base.c create path "M 424.71448 528.79074 L 424.71448 529.79074 L 424.71448 544.79074 L 425.71448 544.79074 L 440.71448 544.79074 L 440.71448 543.79074 L 440.71448 530.79074 L 433.72229 530.79074 L 431.72229 528.79074 L 431.71429 528.7987400000001 L 431.71429 528.79074 L 425.71429 528.79074 L 424.71429 528.79074 Z M 425.71448 529.79074 L 428.71448 529.79074 L 431.30042 529.79074 L 432.30431999999996 530.79465 L 428.28479 534.79074 L 428.28279 534.79074 L 425.71443 534.79074 L 425.71443 529.79074 Z M 430.72229 533.79074 L 439.71448 533.79074 L 439.71448 543.79074 L 425.71448 543.79074 L 425.71448 535.79074 L 427.27698 535.79074 L 428.71448 535.79074 L 428.71448 535.78274 L 428.72247999999996 535.79074 L 430.72247999999996 533.79074 Z" -fill "#3daee9" -matrix {{1.0 0.0} {-0.0 1.0} {-350.0 -482.0}} -stroke {} -tags {Path obj svg}]"
set fe2 ".widgets.c $idfe"

    set g1 [$base.c gradient create linear -stops {{0 "#bababa"} {1 "#454545"}} -lineartransition {0 0 0 1}]
#with blue
    set g1_1 [$base.c gradient create linear -stops {{0 "#55ffff"} {1 "#00aaff"}} -lineartransition {0 0 0 1}]
    update
    set iwbut_y [winfo pixels $base 0.5c]
    set hiwbut [winfo pixels $base 6.61m]
    set delx [winfo pixels $base.c 27m]
    set dely [winfo pixels $base.c 3m]

    set i 0
    set hc $iwbut_y
    set cbut($i) [cbutton new "$base.c" -type rect -x $hc -y $dely -text "crbuttonW"  -fillnormal $g1_1]
    eval "$cbut($i) config -command {butCButton $can $cbut($i)}"
    incr i
    incr hc $delx
    set cbut($i) [cbutton new "$base.c" -type round -x $hc -y $dely -text "crrbuttonW"  -fillnormal red]
    $cbut($i) config -fillnormal tan
    eval "$cbut($i) config -command {butCButton $can $cbut($i)}"

    incr i
    incr hc $delx
    set cbut($i) [cbutton new "$base.c" -type ellipse -x $hc -y $dely -text "cebuttonW"  -fillnormal $g1]
    eval "$cbut($i) config -command {butCButton $can $cbut($i)}"
    incr i
    incr hc $delx
    set g2 [$base.c gradient create radial -method pad -units bbox -stops { { 0.00 #ffff00 0.50} { 1.00 #d42b11 0.80}} -radialtransition {0.50 0.46 0.50 0.25 0.25} ]
    set cbut($i) [cbutton new "$base.c" -type circle -x $hc -y $dely -text "ccbutton" -stroke yellow -fillnormal $g2]
    eval "$cbut($i) config -command {butCButton $can $cbut($i)}"
    incr i
    incr hc $delx
    set squardx [winfo pixels $base 7m]
    set cbut($i) [cbutton new "$base.c" -type square -x $hc -y $dely -text "csbuttonW"  -fillnormal chocolate]
    eval "$cbut($i) config -command {butCButton $can $cbut($i)}"

    incr i
    incr hc $delx
#checkbutton and radiobutton
    set i 0
    set xc $iwbut_y
    set yc [expr {$hiwbut * 2}]
    foreach j {1 2 3} {
	set chbut($j) [cbutton new "$base.c" -type check -x $xc -y $yc -variable "check$j" -text "checkbutton$j"]
	$chbut($j) config -command "butCheck $can"

	set crbut($j) [cbutton new "$base.c" -type radio -x [expr {$xc + $delx * 2}] -y $yc -variable "radioMW" -value $j]
	$crbut($j) config -command "butRadio $can"
	$crbut($j) config -text "radiobutton$j"
#Кнопка с картинкой: есть текст, есть подсказка
	set cibut($j) [ibutton new $base.c -x [expr {$xc + $delx * 4}] -y $yc]
#-image tpblanc	
	set im [subst "$[subst fe$j]"]
	eval "$cibut($j) config -command \"butImage $can\" -image $[subst fe$j]"
	if {$j != 2} {
	    $cibut($j) config -text "ibutton$j" -help "ibutton$j"
	    $cibut($j) config -command "butImage $can \"[set im]\""
	} else {
	    $cibut($j) config -text {} 
puts "IM=$im"
	    $cibut($j) config -command "butImage $can \"[set im]\""
	}
	incr yc [expr {$hiwbut * 1 + $dely }]
    }
#Убираем временной item
#$base.c delete $idfe

#Вставка Стандартных виджетов
    set i 0
    foreach n $orderTK {
	set icon "tp[string map {{ } {}} [string tolower $n]]"
#puts "icon_name=\"$icon\""
	set iw_but($i) [ibutton new $base.c -x $iwbut_y -y [expr {$hiwbut + $yc}] -image $icon -text "" -help "$n"]
#puts "icon_name=\"$icon\" END"
	$iw_but($i) config -command "createWidget $can $n"
	incr iwbut_y $hiwbut
	incr i
    }
#Кнопка выхода
    incr yc [expr {$hiwbut * 3}]


    cbutton create cbut1 "$base.c" -type round -x 20 -y $yc -text "Закрыть окно" -fillnormal $g1
    eval "cbut1 config -command {wm withdraw $base}"
#Кнопка генерации кода
# -width и -height определяют каким должен быть размер изображения, а в srcregion 3-й и 4-й параметр задают действительные размеры изображения

    ibutton create ibut1 $base.c -x [expr {$iwbut_y / 2.0}] -y $yc -image buildgui -width $hiwbut -height $hiwbut  -text "Build GUI" -help "Генерация кода"
    ibut1 config -command "buildGui [set can]"

    incr yc [expr {$hiwbut * 2}]
    "$base.c" configure -height $yc
    wm minsize $base $ww $yc
#Градиентная заливка холста через прямоугольник
    set gradientCloud [$base.c gradient create linear -method pad -units bbox \
    -stops { { 0.05 #87ceeb 1.00} { 0.17 #ffffff 1.00} { 0.29 skyblue 1.00} { 0.87 #ffffff 1.00} { 1.00 skyblue 1.00}} -lineartransition {1.00 0.00 0.75 1.00} ]
    update
    set hp [winfo height $base.c]
    set wp [winfo width $base.c]
#puts "wp=$wp hp=$hp"
    set rect0 [$base.c create prect "0 0 $wp $hp"  -strokelinejoin miter  -fill $gradientCloud -tag "rectFrame"]
    $base.c lower $rect0

}


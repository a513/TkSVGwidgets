proc butCButton {can tbut} {
    set type [$tbut type]
#    puts "butCButton can=$can tbut=$tbut type=$type"
    set cmdfirst [subst {
	global Graphics
	set Graphics(mode) none
       set x \[$can canvasx %x \$Graphics(grid,snap)\]
       set y \[$can canvasy %y \$Graphics(grid,snap)\]
    }]
    switch $type {
	rect {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type rect -x \$x -y \$y  -text cbutton\]}]
	}
	square {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type square -x \$x -y \$y -text csbutton\]}]
	}
	round {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type round -x \$x -y \$y -text crbutton\]}]
	}
	ellipse {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type ellipse -x \$x -y \$y -text cebutton\]}]
	}
	circle {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type circle -x \$x -y \$y -text ccbutton\]}]
	}
	radio {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type radio -x \$x -y \$y -text radiobutton -variable radvar\]}]
	
	}
	check {
    	    set cbut [subst {set cbut \[cbutton new "$can" -type check -x \$x -y \$y -text checkbutton -variable checkvar\]}]
	}
	default {
	    puts "butCButton can=$can tbut=$tbut type=$type unknown"
	}
    }
#    set cmd "$cmdfirst\n$cbut\nbind $can <Button-1> \{\}"
    set cmd "$cmdfirst\n$cbut\nbind $can <Button-1> \{\}\nputs \"Вы создали объект \$cbut\""
#    puts "$cmd"
    eval "bind $can <Button-1> {$cmd}"
    eval "bind $can <Button-3> {}"
    eval "bind $can <B1-Motion> {}"
##############For TKproE #####################
    eval "bind $can <B1-ButtonRelease> {}"
    puts "Вы хотите создать объект класс cbutton типа $type.\nУкажите место его размещения"
}
